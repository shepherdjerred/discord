  "version": "0.2.0",
  "configurations": [
    {
      "name": "start backend",
      "request": "launch",
      "runtimeArgs": ["run-script", "start"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Post Leaderboard",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/league/tasks/leaderboard/oneshot.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "npm: build"
    }
  ]
}
