{
  "name": "@glitter-boys/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc",
    "copy": "cp -R src/league/image/assets/fonts/ dist/league/image/assets/fonts/ && cp -R src/league/image/html/ranked/assets/ dist/league/image/html/ranked/assets/ && cp -R src/league/image/html/lane/assets/ dist/league/image/html/lane/assets/",
    "build": "npm run compile && npm run copy",
    "watch": "tsc --watch",
    "dev": "npx tsc-watch --onSuccess 'npm run start'",
    "eslint:check": "npx eslint src/",
    "prettier:check": "npx prettier --check src/",
    "lint": "npm run lint:check",
    "lint:check": "npm run eslint:check && npm run prettier:check",
    "prettier:fix": "npx prettier --write .",
    "test": "vitest"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.504.0",
    "@glitter-boys/data": "*",
    "@resvg/resvg-js": "^2.6.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^3.3.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^10.4.0",
    "react": "^18.2.0",
    "satori": "^0.10.13",
    "shoukaku": "^4.0.1",
    "sqlite3": "^5.1.7",
    "ts-pattern": "^5.0.6",
    "twisted": "^1.57.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "vitest": "^1.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest-image-snapshot": "^6.4.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@resvg/resvg-js-android-arm-eabi": "2.6.0",
    "@resvg/resvg-js-android-arm64": "2.6.0",
    "@resvg/resvg-js-darwin-arm64": "2.6.0",
    "@resvg/resvg-js-darwin-x64": "2.6.0",
    "@resvg/resvg-js-linux-arm-gnueabihf": "2.6.0",
    "@resvg/resvg-js-linux-arm64-gnu": "2.6.0",
    "@resvg/resvg-js-linux-arm64-musl": "2.6.0",
    "@resvg/resvg-js-linux-x64-gnu": "2.6.0",
    "@resvg/resvg-js-linux-x64-musl": "2.6.0",
    "@resvg/resvg-js-win32-arm64-msvc": "2.6.0",
    "@resvg/resvg-js-win32-ia32-msvc": "2.6.0",
    "@resvg/resvg-js-win32-x64-msvc": "2.6.0"
  }
