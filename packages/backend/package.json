{
  "name": "@glitter-boys/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "compile": "tsc",
    "copy": "cp -R src/league/image/assets/fonts/ dist/league/image/assets/fonts/ && cp -R src/league/image/html/ranked/assets/ dist/league/image/html/ranked/assets/ && cp -R src/league/image/html/lane/assets/ dist/league/image/html/lane/assets/",
    "build": "npm run compile && npm run copy",
    "watch": "tsc --watch",
    "dev": "npx tsc-watch --onSuccess 'npm run start'",
    "eslint:check": "eslint src/",
    "prettier:check": "prettier --check .",
    "lint": "npm run lint:check",
    "lint:check": "npm run eslint:check && npm run prettier:check",
    "prettier:fix": "prettier --write .",
    "test": "vitest"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.465.0",
    "@glitter-boys/data": "*",
    "@resvg/resvg-js": "^2.6.0",
    "@resvg/resvg-js-linux-x64-gnu": "^2.6.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^2.30.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^10.2.5",
    "react": "^18.2.0",
    "satori": "^0.10.11",
    "shoukaku": "^4.0.1",
    "sqlite3": "^5.1.6",
    "ts-pattern": "^5.0.6",
    "twisted": "^1.55.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest-image-snapshot": "^6.3.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.2",
    "@types/react": "^18.2.41",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest-image-snapshot": "^6.3.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
